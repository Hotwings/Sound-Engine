<Window x:Class="Sound_Engine.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:Sound_Engine"
		mc:Ignorable="d"
		Title="MainWindow" Height="450" Width="800">
	<Grid>
		<ListView Margin="50,50,0,50" Width="268" HorizontalAlignment="Left">
			<ListView.View>
				<GridView>
					<GridViewColumn/>
				</GridView>
			</ListView.View>
		</ListView>
		<Grid x:Name="graphContainer" Margin="0,50,50,50" Width="350" HorizontalAlignment="Right">
			<Grid.Effect>
				<DropShadowEffect/>
			</Grid.Effect>
		</Grid>
		<Button Content="+" Margin="0,0,8,150" HorizontalAlignment="Right" Width="20" Click="ZoomGraphY" MinWidth="20" MinHeight="20" Height="20"/>
		<Button Content="-" Margin="0,150,8,0" HorizontalAlignment="Right" Width="20" Click="ZoomGraphY" MinWidth="20" MinHeight="20" Height="20"/>
	</Grid>
</Window>
